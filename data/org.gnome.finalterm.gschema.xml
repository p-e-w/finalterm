<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
	<schema path="/org/gnome/finalterm/" id="org.gnome.finalterm" gettext-domain="finalterm">
		<key type="s" name="terminal-font">
			<default>'Monospace 12'</default>
			<summary>Font used for terminal output</summary>
			<description></description>
		</key>
		<key type="s" name="label-font">
			<default>'Sans Bold 10'</default>
			<summary>Font used for menu button labels</summary>
			<description></description>
		</key>
		<key type="s" name="color-scheme">
			<default>'Default'</default>
			<summary>Color scheme to be used</summary>
			<description></description>
		</key>
		<key type="s" name="theme">
			<default>'Default'</default>
			<summary>Theme to be used</summary>
			<description></description>
		</key>
		<key type="b" name="dark">
			<default>true</default>
			<summary>Use dark theme</summary>
			<description></description>
		</key>
		<key type="d" name="opacity">
			<range min="0" max="1"/>
			<default>0.9</default>
			<summary>Background opacity</summary>
			<description></description>
		</key>
		<key type="i" name="terminal-lines">
			<range min="1" max="2147483647"/>
			<default>24</default>
			<summary>Default number of lines in the terminal</summary>
			<description></description>
		</key>
		<key type="i" name="terminal-columns">
			<range min="1" max="2147483647"/>
			<default>80</default>
			<summary>Default number of columns in the terminal</summary>
			<description></description>
		</key>
		<key type="s" name="shell-path">
			<default>'/bin/bash'</default>
			<summary>Path to the shell executable which is to be run (NOTE: Only bash and zsh are currently supported)</summary>
			<description></description>
		</key>
		<key type="as" name="shell-arguments">
			<default>[]</default>
			<summary>Options to pass to the shell executable</summary>
			<description></description>
		</key>
		<key type="s" name="emulated-terminal">
			<default>'xterm-256color'</default>
			<summary>Value that the TERM environment variable will be set to (NOTE: Changing this variable may affect shell behavior)</summary>
			<description></description>
		</key>
		<key type="i" name="render-interval">
			<range min="1" max="2147483647"/>
			<default>10</default>
			<summary>Minimum time between successive output rendering cycles, in milliseconds</summary>
			<description></description>
		</key>
		<key type="b" name="case-sensitive-autocompletion">
			<default>false</default>
			<summary>Case-sensitive autocompletion</summary>
			<description></description>
		</key>
	</schema>
</schemalist>
